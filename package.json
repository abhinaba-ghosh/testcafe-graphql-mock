{
  "name": "testcafe-graphql-mock",
  "description": "Mock out a GraphQL schema from the client",
  "version": "0.3.1",
  "scripts": {
    "dev": "tsc --declarationMap -w",
    "test-app:run": "cd client && SKIP_PREFLIGHT_CHECK=true npm start",
    "prepublish": "tsc --declarationMap"
  },
  "main": "dist",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "src/",
    "yarn.lock",
    "README.md"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": {
    "name": "Tim Griesser"
  },
  "dependencies": {
    "graphql-tag": "^2.10.4",
    "graphql-tools": "^4.0.3",
    "tslib": "^1.9.3"
  },
  "peerDependencies": {
    "cypress": "^3.1.3",
    "graphql": "^14.0.2"
  },
  "devDependencies": {
    "@types/graphql": "^14.0.3",
    "graphql": "^14.0.2",
    "testcafe": "^1.8.8",
    "ts-loader": "^6.0.4",
    "typescript": "^3.2.2"
  }
}
